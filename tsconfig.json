    "downlevelIteration": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "lib": ["ESNext"],
    "module": "CommonJS",
    "outDir": "out",
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6"
  },
  "include": ["src"]
}
